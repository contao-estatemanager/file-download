{
  "name": "contao-estatemanager/file-download",
  "type": "contao-bundle",
  "description": "File-Download Extension for Contao EstateManager",
  "keywords": [
    "contao",
    "estatemanager",
    "real estate",
    "estate",
    "property",
    "file",
    "import"
  ],
  "homepage": "https://www.contao-estatemanager.com/",
  "license": "proprietary",
  "authors": [
    {
      "name": "Oveleon",
      "homepage": "https://oveleon.de/",
      "role": "Developer"
    },
    {
      "name": "Sebastian Zoglowek",
      "homepage": "https://github.com/zoglo",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "contao/core-bundle": "^4.9",
    "contao-estatemanager/core": "^1.0.29"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoEstateManager\\FileDownload\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "contao-manager-plugin": "ContaoEstateManager\\FileDownload\\ContaoManager\\Plugin"
  }
}
